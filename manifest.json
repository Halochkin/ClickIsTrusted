{
  "name": "ClickIsTrusted",
  "version": "1.0",
  "description": "The devil is in the devtools!",
  "permissions": [
    "debugger",
    "activeTab",
    "declarativeContent",
    "storage",
    "https://*/*",
    "http://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/black_16.png",
      "32": "images/black_32.png",
      "48": "images/black.png",
      "128": "images/black.png"
    }
  },
  "icons": {
    "16": "images/black_16.png",
    "32": "images/black_32.png",
    "48": "images/black.png",
    "128": "images/black.png"
  },
  "manifest_version": 2
}
